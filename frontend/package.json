    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "i18next": "^22.5.0",
    "jwt-decode": "^3.1.2",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.9.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "react-virtualized": "^9.22.5",
    "redux-persist": "^6.0.0",
    "tailwindcss": "^3.3.2",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "build:analyze": "set \"GENERATE_SOURCEMAP=true\" && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "optimize-images": "node scripts/optimizeImages.js",
    "analyze": "npm run build:analyze && source-map-explorer \"build/static/js/*.js\"",
    "analyze:bundle": "webpack-bundle-analyzer build/bundle-stats.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "react-refresh": "^0.14.0",
    "sharp": "^0.32.1",
    "source-map-explorer": "^2.5.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
